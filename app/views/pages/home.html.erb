<h1>Pages#home</h1>
<p><%= flash[:notice] %></p>
<p>Find me in app/views/pages/home.html.erb</p>
<h2>Ressource i18n : https://www.honeybadger.io/blog/rails-i18n/</h2>
<% if !current_user.nil? %>
  <p><%= current_user.email %></p>
<% end %>

<h2>FAQ</h2>
<% @faq.each do |ask| %>
  <details>
    <summary><%= ask["question"] %></summary>
    <p><%= ask["answer"]%></p>
  </details>
<% end %>
